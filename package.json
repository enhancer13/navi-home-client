{
  "name": "navihomeclient",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "np": "np --no-publish",
    "postversion": "react-native-version",
    "android": "yarn react-native run-android",
    "ios": "react-native run-ios",
    "x86_64-ios iphone 14 pro (16.2)": "arch -x86_64 react-native run-ios --simulator \"iPhone 14 Pro Max (16.2)\"",
    "x86_64-ios iPhone 8 Plus": "arch -x86_64 react-native run-ios --simulator \"iPhone 8 Plus\"",
    "x86_64-ios iPad Pro (12.9-inch) (6th generation)": "arch -x86_64 react-native run-ios --simulator \"iPad Pro (12.9-inch) (6th generation)\"",
    "x86_64-ios iPad Pro (12.9-inch) (2nd generation)": "arch -x86_64 react-native run-ios --simulator \"iPad Pro (12.9-inch) (2nd generation)\"",
    "x86_64-ios": "arch -x86_64 react-native run-ios --simulator \"iPhone 14 Pro Max\"",
    "x86_64-pod install": "cd ios && arch -x86_64 pod install --repo-update",
    "pod deintegrate": "cd ios && pod deintegrate",
    "x86_64-pod update": "cd ios && arch -x86_64 pod update",
    "clean cache": "npm cache clean --force && watchman watch-del-all",
    "react-devtools": "react-devtools",
    "upgrade all": "yarn upgrade --latest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:ci": "yarn run test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native-firebase/app": "^18.0.0",
    "@react-native-firebase/auth": "^18.0.0",
    "@react-native-firebase/firestore": "^18.0.0",
    "@react-native-firebase/messaging": "^18.0.0",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-native-windows/cli": "^0.71.6",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.12",
    "color": "^4.2.3",
    "events": "^3.3.0",
    "jwt-decode": "^3.1.2",
    "linq-to-typescript": "^11.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-native": "^0.71.10",
    "react-native-cache": "^2.0.2",
    "react-native-date-picker": "^4.2.13",
    "react-native-device-info": "^10.5.1",
    "react-native-dropdown-picker": "^5.4.4",
    "react-native-error-boundary": "^1.2.3",
    "react-native-event-listeners": "^1.0.7",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.1",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-keychain": "8.1.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-paper": "^5.8.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "3.20.0",
    "react-native-share": "^8.2.2",
    "react-native-tab-view": "^3.3.4",
    "react-native-touchable-scale": "^2.1.2",
    "react-native-vector-icons": "^9.1.0",
    "react-native-video": "6.0.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@react-native-community/cli": "^11.3.2",
    "@react-native-community/cli-debugger-ui": "^11.3.2",
    "@react-native-community/cli-platform-android": "^11.3.2",
    "@react-native-community/cli-platform-ios": "^11.3.2",
    "@react-native-community/cli-plugin-metro": "^11.3.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/color": "^3.0.3",
    "@types/events": "^3.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.12",
    "@types/react-native": "^0.72.2",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^8.43.0",
    "fetch-mock": "^9.11.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.76.6",
    "np": "^8.0.4",
    "react-devtools": "^4.27.8",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "license": "MIT",
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/__mocks__/jest.setup.js",
      "<rootDir>/__mocks__/react-native-gesture-handler.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-native-device-info|react-clone-referenced-element|@react-native-community|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|expo(nent)?|@expo(nent)?/.*|react-native-keychain|@react-native/polyfills|@react-native-firebase/messaging|@react-native-firebase|react-native-video|react-native-gesture-handler)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enhancer13/navi-home-client.git"
  }
}
